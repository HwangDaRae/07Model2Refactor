<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="UploadMapper">

	<!--
	private String fileNo;
	private int fileCount;
	private String[] fileName;
	
	CREATE TABLE Upload_File (
	file_no				NUMBER(20)			NOT NULL,
	fileCount			NUMBER(20)			NOT NULL,
	image_file			VARCHAR2(100)		NOT NULL
	);
	-->

	<resultMap id="uploadSelectMap" type="com.model2.mvc.service.domain.Upload">
		<result property="fileNo" column="file_no" jdbcType="NUMERIC" />
		<result property="fileCount" column="fileCount" jdbcType="NUMERIC" />
		<result property="fileName" column="image_file" jdbcType="VARCHAR" />
	</resultMap>

	<!-- SQL : INSERT -->
	<insert id="addUpload" parameterType="com.model2.mvc.service.domain.Upload">
		INSERT
		INTO users( file_no, fileCount, 
		<foreach collection="">
			image_file1
		</foreach>
		 )
		VALUES ( #{fileNo}, #{fileCount},
		<foreach collection="">
			fileName
		</foreach>
		 )
	</insert>

</mapper>